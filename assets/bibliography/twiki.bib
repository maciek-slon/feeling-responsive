@PhdThesis{Kornuta:2012_phd-twiki,
  Title                    = {Design of Structures and Behaviours of Robot Control Systems Using Active Vision},
  Author                   = {Tomasz Kornuta},
  School                   = {Warsaw University of Technology},
  Year                     = {2012},

  Abstract                 = {This monograph concerns the design of robotic systems utilizing active vision, especially focusing on two aspects: the structure of the control system and the behaviours of the robot enabling effective control of its visual perception. It presents the decomposition of the system structure into several types of subsystems and the definition of their actions in terms of transition functions. A unified method of description of both the structures and the behaviours is proposed. The developed supplementing each other mathematical and graphical notations lead to the specification detailed enough for the deep understanding and implementation of a~given system. Utility of the tool has been validated on three robotic systems emphasizing various aspects of active vision, including switching of different behaviours depending on the task and the state of the environment, changing the way the system processes the visual information and the simultaneous utilization of a~pair of cameras.},
  Grant                    = {NCN2012Kornuta},
  Lang                     = {en},
  Owner                    = {tkornuta},
  Timestamp                = {2013.05.15},
  Twiki                    = {phd},
  Url                      = {https://robotyka.ia.pw.edu.pl/twiki/pub/Publications/AllRestricted/2012_tkornuta_phd.pdf}
}

